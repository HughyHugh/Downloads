public class Animal { 
  public int numOfLegs; 
  public boolean haveEyes; 
  
  public Animal(int legNumber, boolean eyes) { 
    numOfLegs = legNumber; 
    haveEyes = eyes;
  } 
  public int getMethod (numOfLegs) { 
   return numOfLegs; 
  }
  public boolean getMethodTwo (haveEyes) { 
   return haveEyes; 
  }
  
  
} 